<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="tub"
           fetch="ssh://git@gitlab.tubit.tu-berlin.de/" />

  <remote  name="tkn"
           fetch="https://gitlab.tkn.tu-berlin.de/" />

  <remote  name="github"
           fetch="https://github.com/" />

  <default revision="master"
           remote="tkn"
           sync-j="4" />


  <!-- Core -->
  <project name="wishful/wishful_upis.git" remote="tkn" path="upis"/>
  <project name="wishful/wishful_framework.git" remote="tkn" path="framework"/>
  <project name="wishful/wishful_controller.git" remote="tkn" path="controller"/>
  <project name="wishful/wishful_agent.git" remote="tkn" path="agent"/>
  <!-- Libs -->
  <project name="wishful/stream_pipe.git" remote="tkn" path="libs/stream_pipe"/>
  <project name="wishful/pyre.git" remote="tkn" path="libs/pyre"/>
  <project name="wishful/python-tc.git" remote="tkn" path="libs/python-tc"/>
  <project name="phaethon/scapy" remote="github" path="libs/scapy-python3"/>
  <!-- Framework Modules -->
  <project name="wishful/wishful_module_discovery.git" remote="tkn" path="framework_modules/discovery"/>
  <project name="wishful/wishful_module_discovery_pyre.git" remote="tkn" path="framework_modules/discovery_pyre"/>
  <project name="wishful/wishful_module_ptp.git" remote="tkn" path="framework_modules/ptp"/>
  <!-- Controller Modules -->
  <project name="wishful/wishful_module_transaction.git" remote="tkn" path="controller_modules/transaction"/>
  <project name="wishful/wishful_module_handover.git" remote="tkn" path="controller_modules/handover"/>
  <project name="wishful/wishful_module_wireless_topology.git" remote="tkn" path="controller_modules/wireless_topology"/>
  <!-- Agent Modules -->
  <project name="wishful/wishful_module_rules.git" remote="tkn" path="agent_modules/rules"/>
  <project name="wishful/wishful_module_simple.git" remote="tkn" path="agent_modules/simple"/>
  <project name="wishful/wishful_module_iperf.git" remote="tkn" path="agent_modules/iperf"/>
  <project name="wishful/wishful_module_wifi.git" remote="tkn" path="agent_modules/wifi"/>
  <project name="wishful/wishful_module_wifi_ath.git" remote="tkn" path="agent_modules/wifi_ath"/>
  <project name="wishful/wishful_module_ni_sdr.git" remote="tkn" path="agent_modules/ni_sdr"/>
  <project name="wishful/wishful_module_rs_signal_gen.git" remote="tkn" path="agent_modules/rs_signal_gen"/>
  <project name="wishful/wishful_module_spectral_scan_ath9k.git" remote="tkn" path="agent_modules/spectral_scan_ath9k"/>
  <project name="wishful/wishful_module_robot.git" remote="tkn" path="agent_modules/robot"/>
  <project name="wishful/wishful_module_net_linux.git" remote="tkn" path="agent_modules/net_linux"/>
  <project name="wishful/wishful_module_gnuradio.git" remote="tkn" path="agent_modules/gnuradio"/>
  <!-- Examples -->
  <project name="wishful/wishful_examples.git" remote="tkn" path="examples"/>
  <!-- Tests -->
  <project name="wishful/wishful_tests.git" remote="tkn" path="tests"/>
  <!-- Other -->
  <project name="wishful/wishful_mininet.git" remote="tkn" path="mininet"/>
  <project name="wishful/wishful_project_scripts.git" remote="tkn" path="scripts"/>

</manifest>

